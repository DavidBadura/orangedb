{
    "name": "davidbadura/orangedb",
    "description": "Readonly Filesystem Database | Object-File Mapping",
    "keywords": ["database", "filesystem", "yaml", "json", "annotation"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name" : "David Badura",
        "email" : "d.badura@gmx.de"
    }],
    "require": {
        "php": ">=7.4",
        "symfony/yaml": "^3.4|^4.4|^5.0",
        "symfony/finder": "^3.4|^4.4|^5.0",
        "webmozart/json": "^1.2",
        "jms/metadata": "^1.5",
        "doctrine/annotations": "^1.7",
        "timetoogo/pinq": "^3.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.4|^4.4|^5.0",
        "phpunit/phpunit": "9.5.21",
        "symfony/filesystem": "^4.4|^5.0",
        "phpstan/phpstan": "^0.12.33",
        "psalm/phar": "3.18.2"
    },
    "autoload": {
        "psr-4": {
            "DavidBadura\\OrangeDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidBadura\\OrangeDb\\Test\\": "tests"
        }
    }
}
